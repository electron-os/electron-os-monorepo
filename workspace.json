{
  "version": 2,
  "projects": {
    "terminal": {
      "root": "apps/terminal",
      "sourceRoot": "apps/terminal/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/terminal",
            "index": "apps/terminal/src/index.html",
            "main": "apps/terminal/src/main.tsx",
            "polyfills": "apps/terminal/src/polyfills.ts",
            "tsConfig": "apps/terminal/tsconfig.app.json",
            "assets": [
              "apps/terminal/src/favicon.ico",
              "apps/terminal/src/assets"
            ],
            "styles": ["apps/terminal/src/styles.scss"],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/terminal/src/environments/environment.ts",
                  "with": "apps/terminal/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "terminal:build",
            "hmr": true
          },
          "configurations": {
            "production": {
              "buildTarget": "terminal:build:production",
              "hmr": false
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/terminal/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/terminal"],
          "options": {
            "jestConfig": "apps/terminal/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "terminal-e2e": {
      "root": "apps/terminal-e2e",
      "sourceRoot": "apps/terminal-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/terminal-e2e/cypress.json",
            "tsConfig": "apps/terminal-e2e/tsconfig.e2e.json",
            "devServerTarget": "terminal:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "terminal:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/terminal-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "utils": {
      "root": "libs/utils",
      "sourceRoot": "libs/utils/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/utils/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/utils"],
          "options": {
            "jestConfig": "libs/utils/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build": {
          "executor": "@nrwl/node:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/utils",
            "tsConfig": "libs/utils/tsconfig.lib.json",
            "packageJson": "libs/utils/package.json",
            "main": "libs/utils/src/index.ts",
            "assets": ["libs/utils/*.md"]
          }
        }
      }
    },
    "utils-src-lib-style": {
      "root": "libs/utils/src/lib/style",
      "sourceRoot": "libs/utils/src/lib/style/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/utils/src/lib/style/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/utils/src/lib/style"],
          "options": {
            "jestConfig": "libs/utils/src/lib/style/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build": {
          "executor": "@nrwl/node:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/utils/src/lib/style",
            "tsConfig": "libs/utils/src/lib/style/tsconfig.lib.json",
            "packageJson": "libs/utils/src/lib/style/package.json",
            "main": "libs/utils/src/lib/style/src/index.ts",
            "assets": ["libs/utils/src/lib/style/*.md"]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/react"
  },
  "generators": {
    "@nrwl/react": {
      "application": {
        "style": "scss",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "scss"
      },
      "library": {
        "style": "scss",
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "terminal"
}
