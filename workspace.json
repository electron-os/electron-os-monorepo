{
  "version": 2,
  "projects": {
    "terminal": {
      "root": "apps/terminal",
      "sourceRoot": "apps/terminal/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/terminal",
            "index": "apps/terminal/src/index.html",
            "main": "apps/terminal/src/main.tsx",
            "polyfills": "apps/terminal/src/polyfills.ts",
            "tsConfig": "apps/terminal/tsconfig.app.json",
            "assets": [
              "apps/terminal/src/favicon.ico",
              "apps/terminal/src/assets"
            ],
            "styles": ["apps/terminal/src/styles.scss"],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/terminal/src/environments/environment.ts",
                  "with": "apps/terminal/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "terminal:build",
            "hmr": true
          },
          "configurations": {
            "production": {
              "buildTarget": "terminal:build:production",
              "hmr": false
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/terminal/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/terminal"],
          "options": {
            "jestConfig": "apps/terminal/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "terminal-e2e": {
      "root": "apps/terminal-e2e",
      "sourceRoot": "apps/terminal-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/terminal-e2e/cypress.json",
            "tsConfig": "apps/terminal-e2e/tsconfig.e2e.json",
            "devServerTarget": "terminal:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "terminal:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/terminal-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "themes-mac-os-theme": {
      "root": "libs/themes/mac-os-theme",
      "sourceRoot": "libs/themes/mac-os-theme/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/web:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/themes/mac-os-theme",
            "tsConfig": "libs/themes/mac-os-theme/tsconfig.lib.json",
            "project": "libs/themes/mac-os-theme/package.json",
            "entryFile": "libs/themes/mac-os-theme/src/index.ts",
            "external": ["react/jsx-runtime"],
            "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
            "assets": [
              {
                "glob": "libs/themes/mac-os-theme/README.md",
                "input": ".",
                "output": "."
              }
            ]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/themes/mac-os-theme/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/themes/mac-os-theme"],
          "options": {
            "jestConfig": "libs/themes/mac-os-theme/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "themes-mac-os-theme-src-lib-button": {
      "root": "libs/themes/mac-os-theme/src/lib/button",
      "sourceRoot": "libs/themes/mac-os-theme/src/lib/button/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/web:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/themes/mac-os-theme/src/lib/button",
            "tsConfig": "libs/themes/mac-os-theme/src/lib/button/tsconfig.lib.json",
            "project": "libs/themes/mac-os-theme/src/lib/button/package.json",
            "entryFile": "libs/themes/mac-os-theme/src/lib/button/src/index.ts",
            "external": ["react/jsx-runtime"],
            "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
            "assets": [
              {
                "glob": "libs/themes/mac-os-theme/src/lib/button/README.md",
                "input": ".",
                "output": "."
              }
            ]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/themes/mac-os-theme/src/lib/button/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/themes/mac-os-theme/src/lib/button"],
          "options": {
            "jestConfig": "libs/themes/mac-os-theme/src/lib/button/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/react"
  },
  "generators": {
    "@nrwl/react": {
      "application": {
        "style": "scss",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "scss"
      },
      "library": {
        "style": "scss",
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "terminal"
}
