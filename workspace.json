    "themes-mac-os-theme": {
      "root": "libs/themes/mac-os-theme",
      "sourceRoot": "libs/themes/mac-os-theme/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/web:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/themes/mac-os-theme",
            "tsConfig": "libs/themes/mac-os-theme/tsconfig.lib.json",
            "project": "libs/themes/mac-os-theme/package.json",
            "entryFile": "libs/themes/mac-os-theme/src/index.ts",
            "external": ["react/jsx-runtime"],
            "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
            "assets": [
              {
                "glob": "libs/themes/mac-os-theme/README.md",
                "input": ".",
                "output": "."
              }
            ]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/themes/mac-os-theme/**/*.{ts,tsx,js,jsx}"
